local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "UltraFuturisticDupeGUI"
screenGui.ResetOnSpawn = false
screenGui.Parent = playerGui

local themes = {
    {name = "Neon Blue", background = Color3.fromRGB(10, 15, 25), secondary = Color3.fromRGB(20, 30, 50), accent = Color3.fromRGB(0, 150, 255), text = Color3.fromRGB(200, 220, 255)},
    {name = "Cyber Green", background = Color3.fromRGB(5, 20, 15), secondary = Color3.fromRGB(15, 40, 30), accent = Color3.fromRGB(0, 255, 100), text = Color3.fromRGB(180, 255, 200)},
    {name = "Plasma Purple", background = Color3.fromRGB(20, 10, 30), secondary = Color3.fromRGB(40, 20, 60), accent = Color3.fromRGB(180, 0, 255), text = Color3.fromRGB(230, 200, 255)},
    {name = "Void Black", background = Color3.fromRGB(5, 5, 5), secondary = Color3.fromRGB(15, 15, 15), accent = Color3.fromRGB(255, 0, 100), text = Color3.fromRGB(200, 200, 200)},
    {name = "Crimson Tech", background = Color3.fromRGB(30, 5, 5), secondary = Color3.fromRGB(60, 10, 10), accent = Color3.fromRGB(255, 50, 50), text = Color3.fromRGB(255, 200, 200)},
    {name = "Golden Circuit", background = Color3.fromRGB(25, 20, 0), secondary = Color3.fromRGB(50, 40, 0), accent = Color3.fromRGB(255, 200, 0), text = Color3.fromRGB(255, 240, 180)},
    {name = "Aqua Hologram", background = Color3.fromRGB(0, 20, 25), secondary = Color3.fromRGB(0, 40, 50), accent = Color3.fromRGB(0, 200, 255), text = Color3.fromRGB(180, 240, 255)}
}

local currentTheme = 1

local function applyTheme(frame)
    local theme = themes[currentTheme]
    frame.BackgroundColor3 = theme.background
    frame.TitleBar.BackgroundColor3 = theme.secondary
    for _, v in pairs(frame:GetDescendants()) do
        if v:IsA("TextLabel") or v:IsA("TextButton") then
            v.TextColor3 = theme.text
        elseif v:IsA("TextBox") then
            v.BackgroundColor3 = theme.secondary
            v.TextColor3 = theme.text
        end
    end
    for _, button in pairs(frame:GetChildren()) do
        if button:IsA("TextButton") and button.Name ~= "ThemeButton" then
            button.BackgroundColor3 = theme.accent
        end
    end
end

local function createUI(name, size, position, visible)
    local frame = Instance.new("Frame")
    frame.Name = name
    frame.Size = size
    frame.Position = position
    frame.BorderSizePixel = 0
    frame.Visible = visible
    frame.AnchorPoint = Vector2.new(0.5, 0.5)
    frame.BackgroundTransparency = 0.1
    Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 20)
    
    local titleBar = Instance.new("Frame")
    titleBar.Name = "TitleBar"
    titleBar.Size = UDim2.new(1, 0, 0, 50)
    titleBar.BackgroundTransparency = 0.5
    Instance.new("UICorner", titleBar).CornerRadius = UDim.new(0, 20)
    titleBar.Parent = frame
    
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, -60, 1, 0)
    title.Position = UDim2.new(0, 20, 0, 0)
    title.BackgroundTransparency = 1
    title.Text = name == "MainFrame" and "Rdx UI V1.5.9" or "Dupe Configuration"
    title.Font = Enum.Font.GothamBold
    title.TextSize = 22
    title.TextXAlignment = Enum.TextXAlignment.Left
    title.Parent = titleBar
    
    local closeButton = Instance.new("TextButton")
    closeButton.Size = UDim2.new(0, 40, 0, 40)
    closeButton.Position = UDim2.new(1, -45, 0, 5)
    closeButton.BackgroundTransparency = 0.5
    closeButton.Text = "Ã—"
    closeButton.TextColor3 = Color3.new(1, 1, 1)
    closeButton.Font = Enum.Font.GothamBold
    closeButton.TextSize = 28
    Instance.new("UICorner", closeButton).CornerRadius = UDim.new(0, 10)
    closeButton.Parent = titleBar
    
    closeButton.MouseButton1Click:Connect(function()
        local closeTween = TweenService:Create(frame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In), {Size = UDim2.new(0, 0, 0, 0), BackgroundTransparency = 1})
        closeTween:Play()
        closeTween.Completed:Connect(function()
            screenGui:Destroy()
        end)
    end)
    
    frame.Parent = screenGui
    applyTheme(frame)
    return frame
end

local function createButton(parent, text, position, size)
    local button = Instance.new("TextButton")
    button.Size = size
    button.Position = position
    button.Text = text
    button.Font = Enum.Font.GothamSemibold
    button.TextSize = 18
    button.BackgroundTransparency = 0.3
    Instance.new("UICorner", button).CornerRadius = UDim.new(0, 12)
    button.Parent = parent
    return button
end

local function addGlow(button)
    local glow = Instance.new("ImageLabel")
    glow.Size = UDim2.new(1, 40, 1, 40)
    glow.Position = UDim2.new(0, -20, 0, -20)
    glow.BackgroundTransparency = 1
    glow.Image = "rbxassetid://5028857084"
    glow.ImageColor3 = button.BackgroundColor3
    glow.ImageTransparency = 0.7
    glow.Parent = button
end

local function addRippleEffect(button)
    button.MouseButton1Down:Connect(function(x, y)
        local ripple = Instance.new("Frame")
        ripple.Position = UDim2.new(0, x - button.AbsolutePosition.X, 0, y - button.AbsolutePosition.Y)
        ripple.Size = UDim2.new(0, 0, 0, 0)
        ripple.BackgroundColor3 = Color3.new(1, 1, 1)
        ripple.BorderSizePixel = 0
        ripple.AnchorPoint = Vector2.new(0.5, 0.5)
        ripple.Parent = button

        local rippleAnimation = TweenService:Create(ripple, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Size = UDim2.new(2, 0, 2, 0),
            BackgroundTransparency = 1
        })
        rippleAnimation:Play()
        rippleAnimation.Completed:Connect(function()
            ripple:Destroy()
        end)
    end)
end
local mainFrame = createUI("MainFrame", UDim2.new(0, 400, 0, 450), UDim2.new(0.5, 0, 0.5, 0), false)
local secondFrame = createUI("SecondFrame", UDim2.new(0, 400, 0, 430), UDim2.new(0.5, 0, 0.5, 0), false)

local function createHoverAnimation(button)
    button.MouseEnter:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.3), {BackgroundTransparency = 0, TextSize = button.TextSize + 2}):Play()
    end)
    button.MouseLeave:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.3), {BackgroundTransparency = 0.3, TextSize = button.TextSize - 2}):Play()
    end)
end

local dupeButton = createButton(mainFrame, "Dupe Scripts", UDim2.new(0.1, 0, 0.25, 0), UDim2.new(0.8, 0, 0.15, 0))
local bbhButton = createButton(mainFrame, "BBH Hub", UDim2.new(0.1, 0, 0.45, 0), UDim2.new(0.8, 0, 0.15, 0))
local themeButton = createButton(mainFrame, "Themes", UDim2.new(0.1, 0, 0.65, 0), UDim2.new(0.8, 0, 0.15, 0))
local goButton = createButton(secondFrame, "Execute", UDim2.new(0.3, 0, 0.8, 0), UDim2.new(0.4, 0, 0.12, 0))

for _, btn in ipairs({dupeButton, bbhButton, themeButton, goButton}) do
    addGlow(btn)
    addRippleEffect(btn)
    createHoverAnimation(btn)
end

local function addInputField(parent, yPos, labelText)
    local container = Instance.new("Frame")
    container.Size = UDim2.new(0.9, 0, 0, 80)
    container.Position = UDim2.new(0.05, 0, yPos, 0)
    container.BackgroundTransparency = 1
    container.Parent = parent

    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 0, 30)
    label.BackgroundTransparency = 1
    label.Text = labelText
    label.Font = Enum.Font.GothamSemibold
    label.TextSize = 18
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.Parent = container

    local textBox = Instance.new("TextBox")
    textBox.Size = UDim2.new(1, 0, 0, 45)
    textBox.Position = UDim2.new(0, 0, 0, 35)
    textBox.Font = Enum.Font.Gotham
    textBox.TextSize = 16
    textBox.PlaceholderText = "Enter value..."
    textBox.BackgroundTransparency = 0.5
    Instance.new("UICorner", textBox).CornerRadius = UDim.new(0, 10)
    textBox.Parent = container

    return textBox
end

local slotInput = addInputField(secondFrame, 0.2, "Slot Name")
local amountInput = addInputField(secondFrame, 0.45, "Dupe Amount")

local function onButtonClick(button)
    TweenService:Create(button, TweenInfo.new(0.1), {BackgroundColor3 = Color3.new(1, 1, 1), TextColor3 = button.BackgroundColor3}):Play()
    wait(0.1)
    TweenService:Create(button, TweenInfo.new(0.1), {BackgroundColor3 = button.BackgroundColor3, TextColor3 = Color3.new(1, 1, 1)}):Play()
end

dupeButton.MouseButton1Click:Connect(function()
    onButtonClick(dupeButton)
    TweenService:Create(mainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In), {Position = UDim2.new(1.5, 0, 0.5, 0), Rotation = 15}):Play()
    wait(0.5)
    mainFrame.Visible = false
    secondFrame.Visible = true
    secondFrame.Position = UDim2.new(-0.5, 0, 0.5, 0)
    secondFrame.Rotation = -15
    TweenService:Create(secondFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Position = UDim2.new(0.5, 0, 0.5, 0), Rotation = 0}):Play()
end)

bbhButton.MouseButton1Click:Connect(function()
    onButtonClick(bbhButton)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/catblox1346/BBHscript/main/owo?token=$(date%20+%s)"))()
    TweenService:Create(mainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In), {Size = UDim2.new(0, 0, 0, 0), Rotation = 180}):Play()
    wait(0.5)
    screenGui:Destroy()
end)

local function createThemeUI()
    local themeFrame = createUI("ThemeFrame", UDim2.new(0, 300, 0, 400), UDim2.new(0.5, 0, 0.5, 0), true)
    local scrollFrame = Instance.new("ScrollingFrame")
    scrollFrame.Size = UDim2.new(1, -20, 1, -60)
    scrollFrame.Position = UDim2.new(0, 10, 0, 50)
    scrollFrame.BackgroundTransparency = 1
    scrollFrame.BorderSizePixel = 0
    scrollFrame.ScrollBarThickness = 6
    scrollFrame.Parent = themeFrame

    for i, theme in ipairs(themes) do
        local themeButton = createButton(scrollFrame, theme.name, UDim2.new(0.05, 0, 0, (i-1)*70), UDim2.new(0.9, 0, 0, 60))
        themeButton.BackgroundColor3 = theme.accent
        addGlow(themeButton)
        addRippleEffect(themeButton)
        createHoverAnimation(themeButton)
        themeButton.MouseButton1Click:Connect(function()
            currentTheme = i
            applyTheme(mainFrame)
            applyTheme(secondFrame)
            TweenService:Create(themeFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In), {Size = UDim2.new(0, 0, 0, 0), Rotation = -15}):Play()
            wait(0.3)
            themeFrame:Destroy()
        end)
    end
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, #themes * 70)
    return themeFrame
end

themeButton.MouseButton1Click:Connect(function()
    onButtonClick(themeButton)
    local themeFrame = createThemeUI()
    themeFrame.Size = UDim2.new(0, 0, 0, 0)
    themeFrame.Rotation = 15
    TweenService:Create(themeFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Size = UDim2.new(0, 300, 0, 400), Rotation = 0}):Play()
end)

goButton.MouseButton1Click:Connect(function()
    onButtonClick(goButton)
    _G.SlotName = slotInput.Text or 'test'
    _G.DupeAmount = tonumber(amountInput.Text) or 0
    loadstring(game:HttpGet("https://raw.githubusercontent.com/catblox1346/Extra-beta-scripts/refs/heads/main/AutoDupeByName?token=$(date%20+%s)"))()
    
    local executionOverlay = Instance.new("Frame")
    executionOverlay.Size = UDim2.new(1, 0, 1, 0)
    executionOverlay.BackgroundColor3 = Color3.new(0, 0, 0)
    executionOverlay.BackgroundTransparency = 1
    executionOverlay.Parent = screenGui
    
    local executionText = Instance.new("TextLabel")
    executionText.Size = UDim2.new(1, 0, 1, 0)
    executionText.BackgroundTransparency = 1
    executionText.Font = Enum.Font.GothamBold
    executionText.TextSize = 28
    executionText.TextColor3 = Color3.new(1, 1, 1)
    executionText.Text = "Executing..."
    executionText.Parent = executionOverlay
    
    TweenService:Create(executionOverlay, TweenInfo.new(0.5), {BackgroundTransparency = 0.7}):Play()
    TweenService:Create(executionText, TweenInfo.new(0.5), {TextTransparency = 0}):Play()
    wait(1.5)
    TweenService:Create(executionOverlay, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
    TweenService:Create(executionText, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
    wait(0.5)
    executionOverlay:Destroy()
end)

local function makeDraggable(frame)
    local dragToggle, dragInput, dragStart, startPos
    local function updateInput(input)
        local delta = input.Position - dragStart
        TweenService:Create(frame, TweenInfo.new(0.1), {Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)}):Play()
    end
    frame.TitleBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragToggle = true
            dragStart = input.Position
            startPos = frame.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragToggle = false
                end
            end)
        end
    end)
    frame.TitleBar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragToggle then
            updateInput(input)
        end
    end)
end

makeDraggable(mainFrame)
makeDraggable(secondFrame)

mainFrame.Size = UDim2.new(0, 0, 0, 0)
mainFrame.Visible = true
TweenService:Create(mainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Size = UDim2.new(0, 400, 0, 450)}):Play()
